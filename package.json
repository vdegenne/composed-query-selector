{
	"name": "composed-query-selector",
	"type": "module",
	"description": "Like document.querySelectorAll but shadow-dom see-through",
	"version": "2.0.1",
	"devDependencies": {
		"tslib": "^2.8.1",
		"typescript": "^5.6.3",
		"wireit": "^0.14.9"
	},
	"scripts": {
		"build": "wireit"
	},
	"wireit": {
		"build": {
			"command": "tsc --pretty",
			"clean": "if-file-deleted",
			"files": [
				"src",
				"tsconfig.json"
			],
			"output": [
				"lib"
			]
		}
	},
	"files": [
		"./lib/index.js",
		"./lib/index.d.ts"
	],
	"exports": {
		".": {
			"default": "./lib/index.js",
			"types": "./lib/index.d.ts"
		}
	}
}
